{
  "name": "boilerplate-fullstack-auth",
  "type": "module",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "test": "vitest",
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.17.2",
    "express-jwt": "^7.7.5",
    "framer-motion": "^11.5.4",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^2.1.5",
    "knex": "^2.5.1",
    "react-icons": "^5.3.0",
    "sqlite3": "^5.1.6",
    "superagent": "^8.1.2",
    "tailwind-merge": "^2.5.2",
    "three": "^0.168.0",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.10.1",
    "@tanstack/eslint-plugin-query": "^5.12.2",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-query-devtools": "^5.13.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.14",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/superagent": "^8.1.1",
    "@types/supertest": "^6.0.2",
    "@types/three": "^0.168.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.10",
    "esbuild": "^0.18.19",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^22.1.0",
    "nock": "^13.5.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "sass": "^1.50.1",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.11",
    "tsx": "^3.12.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "browserslist": "> 2%, not dead"
}
